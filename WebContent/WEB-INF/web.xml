  </servlet>
  <servlet-mapping>
    <servlet-name>Tokengenerator</servlet-name>
    <url-pattern>/Tokengenerator</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Adminloginhandler</display-name>
    <servlet-name>Adminloginhandler</servlet-name>
    <servlet-class>com.Arkaprabha.Adminloginhandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Adminloginhandler</servlet-name>
    <url-pattern>/Adminloginhandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Addpartyhandler</display-name>
    <servlet-name>Addpartyhandler</servlet-name>
    <servlet-class>com.Arkaprabha.Addpartyhandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Addpartyhandler</servlet-name>
    <url-pattern>/Addpartyhandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>mainServlet</display-name>
    <servlet-name>mainServlet</servlet-name>
    <servlet-class>com.Arkaprabha.mainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mainServlet</servlet-name>
    <url-pattern>/mainServlet</url-pattern>
  </servlet-mapping>
</web-app>