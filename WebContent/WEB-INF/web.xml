    <description>to handle checking and creation of voter token </description>
    <display-name>Tokenhandler</display-name>
    <servlet-name>Tokenhandler</servlet-name>
    <servlet-class>com.Arkaprabha.Tokenhandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Tokenhandler</servlet-name>
    <url-pattern>/Tokenhandler</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Tokengenerator</display-name>
    <servlet-name>Tokengenerator</servlet-name>
